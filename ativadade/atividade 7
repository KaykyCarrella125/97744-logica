import os 

os.system("clear") #limpa o terminal. 

#solicitando dados (entrada)

idade = int(input("digite sua idade: "))


if idade > 18:
     print("é obrigatorio")

if idade > 65:
    print("não obrigatorio a votar")


if idade == 16 and 17:
     print("voto opcional")


if idade < 16:
     print("não podem votar")