import os

os.system("clear") #limpar o terminal

#solicitando dados (entrada)
valor=int(input("digite o seu valor, MAIOR que 10:"))

#verificando (processamento)

if valor >10:
   print("MAIOR que 10!")
else: 
    print("NÃO É MAIOR QUE 10!:")
    #Exibindo dados (saida)
    
    
    print("==fim==")

