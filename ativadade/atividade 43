import os
os.system("cls || clear")

def preço(pro1):
    if pro1 < 100:
        resultado = pro1 * 1.10
        return resultado
    else:
        resultado = pro1 * 1.20
        return resultado

pro1 = float(input("digite o valor da sua compra: "))

imposto = preço(pro1)

print(f"Valor da sua compra R${imposto:.2f}")